{
  "manifest_version": 2,
  "name": "SpotWikiEdit",
  "version": "1.0",
  "description": "A SpotWikiEdit",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://en.wikipedia.org/wiki/Wikipedia:Sandbox"
      ]
    }
  ],
  "icons": {
    "48": "icons/logo-32.png",
    "96": "icons/logo-32.png"
  },
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "SpotWikiEdit"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}